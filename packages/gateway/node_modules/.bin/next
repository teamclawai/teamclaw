#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chenillen/Codes/Projects/MarvisLLC/teamclaw/node_modules/.pnpm/next@15.5.12_react-dom@19.2.4_react@19.2.4__react@19.2.4/node_modules/next/node_modules:/Users/chenillen/Codes/Projects/MarvisLLC/teamclaw/node_modules/.pnpm/next@15.5.12_react-dom@19.2.4_react@19.2.4__react@19.2.4/node_modules:/Users/chenillen/Codes/Projects/MarvisLLC/teamclaw/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/chenillen/Codes/Projects/MarvisLLC/teamclaw/node_modules/.pnpm/next@15.5.12_react-dom@19.2.4_react@19.2.4__react@19.2.4/node_modules/next/node_modules:/Users/chenillen/Codes/Projects/MarvisLLC/teamclaw/node_modules/.pnpm/next@15.5.12_react-dom@19.2.4_react@19.2.4__react@19.2.4/node_modules:/Users/chenillen/Codes/Projects/MarvisLLC/teamclaw/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../next/dist/bin/next" "$@"
else
  exec node  "$basedir/../next/dist/bin/next" "$@"
fi
